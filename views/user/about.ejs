        .service-card:hover::before {
            transform: scaleX(1);
        }

        .service-icon {
            width: 90px;
            height: 90px;
            background: linear-gradient(135deg, #000000, #333333);
            border-radius: 50%;
            display: flex;
            align-items: center;
            justify-content: center;
            margin-bottom: 2rem;
            font-size: 2.2rem;
            color: white;
            transition: all 0.3s ease;
